{
  "enums": {
    "Shiftlight_Mode": {
      "LEFT_RIGHT": 1,
      "BOTH_SIDES": 2
    },
    "Button_Name": {
      "PIT": 1,
      "FUEL": 2,
      "FCK": 3,
      "STINT": 4,
      "ALARM": 5
    },
    "Button_State": {
      "PRESSED": 1,
      "DEPRESSED": 2
    }
  },
  "messages": {
    "Proto_Car_Sensor": {
      "fields": {
        "temp": { "tag": 1, "type": "uint32", "rule": "optional", "default": 0 },
        "preassure": { "tag": 2, "type": "double", "rule": "optional", "default": 0 }
      }
    },
    "Proto_Odb2_Data": {
      "fields": {
        "rpm": { "tag": 1, "type": "uint32", "rule": "optional" },
        "coolant": { "tag": 2, "type": "int32", "rule": "optional" }
      }
    },
    "Proto_Shiftlight_Config": {
      "fields": {
        "mode": { "tag": 1, "type": "Shiftlight_Mode", "rule": "optional" },
        "rpm_red_flash": { "tag": 2, "type": "int32", "rule": "optional" },
        "brightness": { "tag": 3, "type": "int32", "rule": "optional" },
        "rpm_limits": { "tag": 4, "type": "int32", "rule": "repeated", "max_count": 10 }
      }
    },
    "Proto_Button": {
      "fields": {
        "name": { "tag": 1, "type": "Button_Name", "rule": "optional" },
        "state": { "tag": 2, "type": "Button_State", "rule": "optional" }
      }
    },
    "Proto_Mcu_Data": {
      "fields": {
        "network_time_adjustment": { "tag": 1, "type": "uint32", "rule": "optional" },
        "send_timestamp": { "tag": 2, "type": "uint32", "rule": "optional" },
        "water": { "tag": 3, "type": "Proto_Car_Sensor", "rule": "optional" },
        "oil": { "tag": 4, "type": "Proto_Car_Sensor", "rule": "optional" },
        "gas": { "tag": 5, "type": "Proto_Car_Sensor", "rule": "optional" },
        "gas_warn": { "tag": 6, "type": "Proto_Car_Sensor", "rule": "optional" },
        "oil_warn": { "tag": 7, "type": "Proto_Car_Sensor", "rule": "optional" },
        "odb2": { "tag": 8, "type": "Proto_Odb2_Data", "rule": "optional" },
        "shiftlight_config": { "tag": 9, "type": "Proto_Shiftlight_Config", "rule": "optional" },
        "pitstop_since": { "tag": 10, "type": "int64", "rule": "optional" }
      }
    }
  }
}
